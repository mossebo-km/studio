        "babel-polyfill": "^6.26.0",
        "browser-sync": "^2.24.5",
        "browser-sync-webpack-plugin": "^2.2.2",
        "cross-env": "^5.1.6",
        "jquery": "^3.2",
        "laravel-mix": "^2.0",
        "lodash": "^4.17.10",
        "normalize.css": "^8.0.0",
        "vue": "^2.5.16"
    }
